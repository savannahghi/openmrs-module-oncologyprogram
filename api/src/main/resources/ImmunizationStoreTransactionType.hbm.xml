<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.mchapp.model">
    <class name="ImmunizationStoreTransactionType" table="immunization_store_transaction_type" batch-size="500">
        <id name="id" type="int" column="id" unsaved-value="0">
            <generator class="native" />
        </id>
        <property name="transactionType" column="transaction_type" length="255" />
    </class>
</hibernate-mapping>